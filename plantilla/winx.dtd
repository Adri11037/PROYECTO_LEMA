<!ELEMENT COLECCION (INTRODUCCION,DESCRIPCION_SERIE,LOGO,IVA,TEMPORADAS,PERSONAJES,CRIATURAS,PRODUCTOS,VESTUARIOS)>
<!ELEMENT INTRODUCCION (#PCDATA)>
<!ELEMENT DESCRIPCION_SERIE (PARRAFO*)>
<!ELEMENT PARRAFO (#PCDATA)>
<!ELEMENT LOGO (#PCDATA)>
<!ELEMENT IVA (#PCDATA)>
<!ELEMENT TEMPORADAS (TEMPORADA+,EXISTENCIAS_TOTALES)>
