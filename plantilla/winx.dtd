<!ELEMENT CARTA (NOMBRE,DESCRIPCION_PERSONAJE,IMG,PRECIO,DESCUENTO,EXISTENCIAS,WEB)>
<!ATTLIST CARTA id IDREF #REQUIRED>
<!ELEMENT DESCRIPCION_PERSONAJE (#PCDATA)>
<!ELEMENT ESPECIALISTAS (CARTA+)>
<!ELEMENT BRUJAS (CARTA+)>
<!ELEMENT OBJETOS_MAGICOS (OBJETO+)>
<!ELEMENT OBJETO (NOMBRE,IMG,DESCRIPCION,PRECIO,DESCUENTO,EXISTENCIAS,WEB)>
<!ATTLIST OBJETO tipo (accesorio|objeto) #REQUIRED>
<!ELEMENT VESTUARIOS (ROPA+,EXISTENCIAS_TOTALES)>
<!ELEMENT ROPA (NOMBRE,DESCRIPCION,IMG,PRECIO,DESCUENTO,EXISTENCIAS,WEB)>